<div>

<% if @letter.letter_type.name.to_s == "Cover Letter"  %>
   
  <%= render 'cover_letter' %>

 <% elsif @letter.letter_type.name.to_s == "Networking"  %>

  <%= render 'networking' %>

<% elsif @letter.letter_type_id == 12 %>

  <%= render 'thank_you' %>

  <% elsif @letter.letter_type_id == 9 %>

  	<%= render 'feedback' %>

  	 <% else %>

  	 <%= render 'progress' %>

 <% end %>


  

   
 
</div>

