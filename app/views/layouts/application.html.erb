<!DOCTYPE html>
<html>
<head>
  <title>GhostwriteIo</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag 'application','http://fonts.googleapis.com/css?family=Raleway+Dots' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'assets/javascripts/bower_components/typed.js' %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class =" navbar navbar-fixed-top">
		<div class="navbar-inner">
 			<div class="container">
 				<a href="#" class="brand"> Ghostwrite </a>
 				<ul class="nav">
 					<li><%= link_to "All Ghostwrites", ghostwrites_path%> </li>
 				</ul>
 				<ul class="nav pull-right">
 					<li><%= link_to current_user.first_name %><%= link_to current_user.last_name %></li>

 				</ul>
 			</div>
		</div>	
	</div>
	
<%= yield %>
	
</body>
</html>
